    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.haroofquizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HaroofQuizApp">
        <activity
            android:name=".ResultPage"
            android:exported="false" />
        <activity
            android:name=".GhunnaContent"
            android:exported="false" />
        <activity
            android:name=".LisaveyahContent"
            android:exported="false" />
        <activity
            android:name=".Niteeyah"
            android:exported="false" />
        <activity
            android:name=".TarfiyahContent"
            android:exported="false" />
        <activity
            android:name=".sharjariyahContent"
            android:exported="false" />
        <activity
            android:name=".LahatiyahContent"
            android:exported="false" />
        <activity
            android:name=".HalqiyahContent"
            android:exported="false" />
        <activity
            android:name=".QuizPage"
            android:exported="false" />
        <activity
            android:name=".PracticePage"
            android:exported="false" />
        <activity
            android:name=".LearnPage"
            android:exported="false" />
        <activity
            android:name=".AppFront"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ManifestOrder,ScopedStorage,WrongManifestParent" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"
        tools:ignore="WrongManifestParent" />
</manifest>